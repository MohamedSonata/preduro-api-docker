{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "orderTotal": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "orderStatus": {
      "type": "enumeration",
      "enum": [
        "inProccess",
        "Received",
        "WithCarrier",
        "OutForDelivery",
        "Delivered"
      ],
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "deliveredData": {
      "type": "datetime",
      "default": "2022-07-12T22:05:00.000Z",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "itemsPrice": {
      "type": "decimal",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "shippingFee": {
      "type": "biginteger",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "delivereyType": {
      "type": "enumeration",
      "enum": [
        "Standard",
        "Fast",
        "SameDay"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "order_products": {
      "type": "component",
      "repeatable": true,
      "component": "cart-product-qan.cart-product-qan",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "billing_address": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "orderYear": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "orderNumber": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "unique": true
    }
  }
}

{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Products",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "brand": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "text",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "discount": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "old_price": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "quantity": {
      "type": "integer",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "new",
        "used",
        "consists"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "seller_data": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::seller-data.seller-data",
      "inversedBy": "products"
    },
    "colors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::color.color"
    },
    "benefits": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "price": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::categorie.categorie",
      "inversedBy": "products"
    },
    "serial_number": {
      "type": "biginteger",
      "required": true,
      "unique": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "weight": {
      "type": "enumeration",
      "enum": [
        "GM200",
        "GM400",
        "GM500",
        "GM750",
        "Kg1",
        "Kg1.5",
        "Kg2",
        "Kg2.5",
        "Kg3",
        "Kg3.5",
        "Kg4",
        "Kg4.5",
        "Kg5",
        "Kg5.5",
        "Kg6"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "Small",
        "Medium",
        "Large",
        "XLarge",
        "XXLarge",
        "XXXLarge",
        "XXXXLarge",
        "XXXXXLarge",
        "XXXXXXLarge"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
